{
  "name": "Quick cookie eraser",
  "version": "0.2",
  "manifest_version": 2,
  "description": "Quickly delete cookies of the current tab domain",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["uri_parser.js", "eraser.js"]
  },
  "permissions": [
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
