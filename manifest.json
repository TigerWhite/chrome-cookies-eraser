{
  "name": "Localhost cookies eraser",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Quickly delete your localhost cookies",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["eraser.js"]
  },
  "permissions": [
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
